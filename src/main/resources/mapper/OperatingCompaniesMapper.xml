<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="maint.mapper.OperatingCompaniesMapper">

    <select id="getAllOperatingCompaniesForSelectOption" resultType="OperatingCompanySelectDto">
        SELECT "operating_company_id"
             , "business_corp_id"
             , "operating_name"
          FROM "operating_companies"
         WHERE "del_flag"       = 0
           AND "available_flag" = 1
    </select>


</mapper>
